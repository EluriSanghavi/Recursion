
//iteration

#include<iostream>
using namespace std;
int main()
{
    int n,fact=1;
    cout<<"enter n";
    cin>>n;
    for(int i=1;i<=n;i++)
    {
       fact=fact*i;
    }
    cout<<fact;
}

//recursion


#include<iostream>
using namespace std;
int n,i;
int fact(int i)
{
if(i<=1)
{
return i;
}
return i*fact(i-1);
}
int main()
{
int n;
cout<<"enter n";
cin>>n;
fact(1);
cout<<fact(n);
}



